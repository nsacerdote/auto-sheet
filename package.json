    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "xlsx": "^0.18.0"
  },
  "peerDependencies": {
    "file-saver": "^2.0.0",
    "xlsx": "^0.18.0",
    "csv-stringify": "^6.0.0"
